
<%= form_for @some_quiz do |f| %>
    <% @quiz_questions.each do |question| %>
        <%= f.fields_for :quiz_answers, question do |ff| %>
            <%= question.description %><br>
            <%= ff.label :answer %><br>
            <%= ff.text_field :answer, :value => "unesi odg" %><br>
        <% end %>
    <% end %>    
    <%= f.submit %>
<% end %>
    
    
