
<%= form_for @some_quiz do |f| %>
        <%= f.fields_for :quiz_answers do |ff| %>
            <%= @quiz_questions[ff.index].description %><br>
            <%= ff.label :answer %><br>
            <%= ff.text_area :answer, :value => "" %><br>
        <% end %>
    <%= f.submit %>
<% end %>
    
    
